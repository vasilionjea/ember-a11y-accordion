{{yield (hash
  isExpanded=(readonly isExpanded)
  header=(component "accordion-header"
    id=sharedState.headerId
    triggerId=sharedState.triggerId
    panelId=sharedState.panelId
    isCollapsible=isCollapsible
    isExpanded=(readonly isExpanded)
    isDisabled=(readonly isDisabled)
    toggle=(action toggle sharedState)
  )
  panel=(component "accordion-panel"
    id=sharedState.panelId
    triggerId=sharedState.triggerId
    isCollapsible=isCollapsible
    isExpanded=(readonly isExpanded)
  )
)}}
